{
  "name": "qa-code-test",
  "version": "1.0.0",
  "description": "QA Code Quiz",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "webpack-dev-server --open",
    "start:test": "webpack-dev-server --config webpack.test.config.js",
    "test": "yarn test:unit && yarn test:end-to-en",
    "test:end-to-end": "start-server-and-test 'yarn start:test' http://localhost:8080 'cypress run'",
    "start-and-test": "concurrently \"yarn start\" \"npx cypress run\"",
    "test:unit": "jest",
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report",
    "test:allure": "yarn test:end-to-end && yarn allure:generate && yarn allure:open",
    "testwithreport": "concurrently \"yarn start\" \"npx cypress run && yarn allure:generate && yarn allure:open\""
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@mdi/font": "^4.1.95",
    "@mdi/js": "^4.1.95",
    "@mdi/react": "^1.2.1",
    "@meldcx/agent": "1.0.31",
    "cypress": "^14.3.2",
    "express": "^4.17.1",
    "howler": "^2.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-spinners": "^0.6.1",
    "serialize-javascript": "2.1.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@jest/types": "^29.6.3",
    "@shelex/cypress-allure-plugin": "^2.41.1",
    "@types/howler": "^2.1.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.10.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
